{
    "name": "di-ipv-common-lambdas",
    "scripts": {
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "unit": "jest --silent --testPathIgnorePatterns tests/unit/handlers/contract/*.test.ts",
        "test": "npm run unit --",
        "test:coverage": "npm run unit -- --coverage",
        "sam:validate": "cd ../../infrastructure/lambda && sam validate && sam validate --lint",
        "sam:build": "npm run sam:validate && sam build --template ../../infrastructure/lambda/template.yaml --cached --parallel",
        "start:contract": "cd tests/unit/handlers/contract && ts-node ./app.ts",
        "test:contract": "wait-on tcp:127.0.0.1:3000; jest --silent tests/unit/handlers/contract/*.test.ts --runInBand",
        "test:contract:ci": "npm-run-all -p -r start:contract test:contract",
        "deploy": "../../deploy.sh",
        "compile": "tsc"
    },
    "dependencies": {
        "@aws-lambda-powertools/logger": "1.7.0",
        "@aws-lambda-powertools/metrics": "1.7.0",
        "@aws-lambda-powertools/parameters": "1.17.0",
        "@aws-lambda-powertools/tracer": "1.7.0",
        "@aws-sdk/client-dynamodb": "3.363.0",
        "@aws-sdk/client-kms": "3.363.0",
        "@aws-sdk/client-sqs": "3.363.0",
        "@aws-sdk/client-ssm": "3.363.0",
        "@aws-sdk/credential-providers": "3.363.0",
        "@aws-sdk/lib-dynamodb": "3.150.0",
        "@middy/core": "4.2.8",
        "@pact-foundation/pact": "^13.1.2",
        "esbuild": "0.17.15",
        "express": "^4.19.2",
        "express-asyncify": "^3.1.0",
        "express-rate-limit": "^7.4.0",
        "jose": "4.13.1"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.114",
        "@types/express": "^4.17.21",
        "@types/jest": "29.5.0",
        "@types/node": "18.15.11",
        "@typescript-eslint/eslint-plugin": "6.13.2",
        "@typescript-eslint/parser": "6.13.2",
        "esbuild-jest": "0.5.0",
        "eslint": "8.55.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-prettier": "5.0.1",
        "jest": "29.5.0",
        "npm-run-all": "^4.1.5",
        "prettier": "3.1.0",
        "ts-jest": "29.1.0",
        "ts-node": "10.9.1",
        "typescript": "5.3.2",
        "wait-on": "^7.2.0"
    }
}
